    <TextView
        android:id="@+id/tv_8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/colorAccent"
        android:padding="10dp"
        android:text="8.内存测试"/>


    <TextView
        android:id="@+id/tv_9_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/colorAccent"
        android:padding="10dp"
        android:text="9.1 打开查看fps流畅度"/>

    <TextView
        android:id="@+id/tv_9_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/colorAccent"
        android:padding="10dp"
        android:text="9.2 关闭查看fps流畅度"/>

    <TextView
        android:id="@+id/tv_9_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
